@model List<CategoryModel>

<h1>Categories</h1>
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>Category Name</th>
			<th>Display Order</th>
			<th> </th>
			<th> </th>
		</tr>
	</thead>
	<tbody>
		@foreach (var obj in Model)
		{
			<tr>
				<td>@obj.Name</td>
				<td>@obj.DisplayOrder</td>
				<td><a>Edit</a></td>
				<td><a>Delete</a></td>
			</tr>
		}
	</tbody>
</table>